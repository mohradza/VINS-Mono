<launch>

   <!-- Launch the sensors used for VINS and Octomap -->
   <include file="/home/ohrad/catkin_ws/src/VINS-Mono/vins_estimator/launch/vins_sensors.launch" />
    
   <!-- Start the VINS-mono node -->
   <node pkg="timed_roslaunch" type="timed_roslaunch.sh" name="timed_roslaunch"
         args="8 vins_estimator realsense_infra.launch" output="screen"/>

   <!-- Transforms from Global to World -->
   <node pkg="tf" type="static_transform_publisher" name="global2target" args="0 0 -0.5 0 0 0 1 /global /calibu_target  100"/>
   <include file="$(find calibu_tracker_ros)/launch/run_calibu_tracker_ros.launch"/>
   <node pkg="tf" type="static_transform_publisher" name="odom2cam" args="0 0 0 0 0 0 1 /camera_link_t0 /pstack_t0 100"/>
   <node pkg="tf" type="static_transform_publisher" name="pstack2vins" args="0 0 0 -1 0 0 0  /pstack_t0 /body_t0  100"/>
   <node pkg="tf" type="static_transform_publisher" name="vins2world" args="0 0 0 0 0 0 1  /body_t0 /world  100"/>

   <!-- Transforms from World to Camera -->
   <node pkg="tf" type="static_transform_publisher" name="cam2odom" args="0 0 0 0 0 0 1 /pstack /camera_link  100"/>
   <node pkg="tf" type="static_transform_publisher" name="depth2odom" args=".05 0 0 0 0 0 1  /pstack /depth_link  100"/>
   <node pkg="tf" type="static_transform_publisher" name="vins2pstack" args="0 0 0 1 0 0 0  /body /pstack  100"/>

   <include file="/home/ohrad/catkin_ws/src/octomap_mapping/octomap_server/launch/vins_octomap.launch" />

</launch>
